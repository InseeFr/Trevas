{
  "gitbook": "3.2.2",
  "title": "Trevas",
  "plugins": [
    "edit-link",
    "prism",
    "-highlight",
    "github",
    "anchorjs",
    "image-captions",
    "include-codeblock",
    "collapsible-chapters",
    "advanced-emoji"
  ],
  "root": "./",
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/InseeFr/VTL-Tools/tree/master/docs",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/InseeFr/VTL-Tools.git"
    },
    "theme-default": {
      "styles": {
        "website": "gitbook.css"
      }
    },
    "collapsible-chapters": {}
  }
}
